{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "http://localhost:1234",
      "webRoot": "${workspaceFolder}",
      "breakOnLoad": true,
      "pathMapping": {
        "__parcel_source_root": "${workspaceFolder}"
      },
      "sourceMapPathOverrides": {
        "../*": "${webRoot}/*",
      }
    }
  ]
}